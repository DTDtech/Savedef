{
  "manifest_version": 3,
  "key": "",
  "name": "savedef",
  "version": "1.0",
  "description": "Save definitions and images of key words",
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "identity",
    "storage",
    "tabs",
    "contextMenus",
    "scripting",
    "activeTab",
    "cookies" 
  ],
  "oauth2": {
    "client_id": "1028764706692-bqc3qjacdf43i8jvkp3ot04itpmfnnoa.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "action": {
    "default_title": "savedef",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "https://accounts.google.com/"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}